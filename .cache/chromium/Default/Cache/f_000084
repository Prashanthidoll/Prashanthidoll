        function CheckUpgradedUser(){
            $('#IsMockUpgradedUserModal').modal({
                backdrop: 'static',
                keyboard: false
            });
        }

        function isalphabet(evt) {
            var charCode = (evt.which) ? evt.which : event.keyCode
            if (charCode > 32 && (charCode < 65 || charCode > 90 || charCode == 31) &&
                  (charCode < 97 || charCode > 122)) {
                if ((charCode == 46) || charCode == 39 || charCode == 45 || charCode == 95) { return true; }
                else { return false; }
            }
            return true;
        }
        function SubCatDetail() {
            var islogin = 'True';
            var usertype = 'ST';
            if (islogin.toLowerCase() == "true" && usertype.toLocaleLowerCase() == "st") {
                $("#ModelMockTest").modal({
                    backdrop: 'static',
                    keyboard: false
                });
                return false;
            }
            else if (islogin.toLowerCase() == "true" && usertype.toLocaleLowerCase() == "cp" || usertype.toLocaleLowerCase() == "tc") {
                document.getElementById("A11").href = "/";
                if (usertype.toLocaleLowerCase() == "tc") {
                    $("#divinfo").html("This test is not for Recruiter !");
                }
                else {
                    $("#divinfo").html("This test is not for Recruiter !");
                }

                $('#Informationmodal').modal({
                    backdrop: 'static',
                    keyboard: false
                });
            }
            else {
                //$("#MasterLogin").modal({
                //    backdrop: 'static',
                //    keyboard: false
                //});

                //return false;
                window.location = "https://www.youth4work.com/Users/ST/Register?returnurl=https%3a%2f%2fwww.prep.youth4work.com%2fPlacement-Papers%2fTCS-Test";

            }
        }

