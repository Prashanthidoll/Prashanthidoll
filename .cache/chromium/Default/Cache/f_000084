    <script>

        var islogin = 'True';


        $(document).ready(function () {

            if ($(".visible-xs").css("display") == "block") {
                $("#MobileDiv").html($("#accordion").html());
                $("#MobileAppDiv").html($("#AppPartialView").html());
                $("#MobileDiv").show();
                $("#MobileAppDiv").show();
                $("#accordion").hide();
                $("#AppPartialView").hide();
            }

            $("#testname")[0].innerHTML = document.getElementById('bcLavel3').innerHTML;

                $('#IsMock').click(function () {
                    if ($(this).is(':checked')) {
                        $('#Durationtextbox').css("display", "block");
                    }
                    else{
                        $('#Durationtextbox').css("display", "none");
                    }
                });
        })

        function CheckQuestion(_noofquestion) {
            var islogin = 'True';
            var logintype = 'ST';
            if (islogin.toLowerCase() == "true") {
                if (logintype.toLocaleLowerCase() != "st") {
                    document.getElementById("A11").href = window.location.href;
                    $("#divinfo").html("This test is only for youth!");
                    $("#Informationmodal").modal({ backdrop: 'static', keyboard: false });
                    return false;
                }
                else if (_noofquestion.toLocaleLowerCase().trim() == "0") {

                    $("#myModalLabel").html("Limited Questions");
                    $("#divinfo").html('<div>See your test report - <a href="/MyReport"> My Report </a></div><div>Upgrade for more question.</div>');
                    $("#A11").html("Continue");
                    $("#A11").attr('href', 'https://www.prep.youth4work.com/Upgrade').attr('class', 'btn btn-primary');
                    $('#Informationmodal').modal({
                        backdrop: 'static',
                        keyboard: false
                    });
                    event.preventDefault();
                    return false;
                }
                return true;
            }


        }
        function getrecord(cdiv, sender) {
            $(".collesdiv").hide(); $("#" + cdiv).show("slow");
            var _seldiv = sender.childNodes[1].innerHTML.toString().replace("/test/g", "_test_").replace("/Test/g", "_Test_").replace("/./g", "_").replace(/-/g, ",").replace(/ /g, "-").replace("/+/g", "-").replace("/-in-/g", "_in_") + '-Test';
            setTimeout(function () { window.location.href = _seldiv; }, 50);

        }
     
    </script>

        <script>

            $(function () {
                var _catt = "Placement Papers";
                var _subcatt = "TCS";
                $.get("/PrepBoard/getMockTestsBysubjectid", { cat: _catt, subcat: _subcatt, lang: '' }, function (data) {
                    if (data != null && data != "undefined" && data != "") {
                        $("#divMockTests").html(data);
                    }
                    else {
                    }
                });
            });



            function savedata(_id, _lblid, _slotid) {
                $("#ppLoadingKP").show();
                var userId = '3849730'
                var prepid = _id.split('_')[1];
                var slotno = $("#" + _slotid).val(); // && 32700
                if (slotno > 0) {
                    $.post("/PrepTest/UpdateSlot", { userid: userId, id: prepid, slot: slotno }, function (resp) {
                        if (resp != "0") {
                            $("#ppLoadingKP").hide();
                            $("#Quesnumber").text($("#Quesnumber").text() - $("#que_" + prepid).text() + parseInt(resp));
                            $("#que_" + prepid).text(resp);
                            hideEdit(_id, _lblid);
                        }
                        else {
                            alert("Something went wrong!");
                        }
                    })
                }
                else {
                    alert("Slot must greater than 0!");
                    $("#ppLoadingKP").hide();
                }

            }


            function ShowEdit(id, lblid) {
                $("#" + id).show(); $("#" + lblid).hide();

            }

            function hideEdit(id, lblid) {
                $("#" + id).hide(); $("#" + lblid).show();
            }

            function slide(pnl) {
                $("#divtest_" + pnl).slideToggle();
                var oldtext = $("#imgsubcat_" + pnl).attr("src");
                if (oldtext == "/img/plus_icon.png") {
                    $("#imgsubcat_" + pnl).attr("src", "/img/negtiv_icon.png");
                }
                else {
                    $("#imgsubcat_" + pnl).attr("src", "/img/plus_icon.png");
                }
            }



        </script>
