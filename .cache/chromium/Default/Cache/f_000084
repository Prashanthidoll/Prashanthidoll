    <script type="text/javascript">
        var specialKeys = new Array();
        specialKeys.push(8); //Backspace

        function IsNumeric(e) {
            var keyCode = e.which ? e.which : e.keyCode
            var ret = ((keyCode >= 48 && keyCode <= 57) || specialKeys.indexOf(keyCode) != -1);
            return ret;
        }
        function onFocus(curr) {
            if ($(curr).attr('data-infocus') == "0") {
                $(curr).attr('data-infocus', '1');
                $(curr).attr('placeholder', '');
            }
        }
        function outFocus(curr) {
            if ($(curr).attr('data-infocus') == "1") {
                $(curr).attr('data-infocus', '0');
                $(curr).attr('placeholder', 'ENTER PHONE NUMBER');
            }
        }
        function checkvalidmobilenum() {
            var mob = $("#customerNo").val().trim();
            if ((mob == "") || (mob.length != 10)) {
                $("#status_myModalLabel").css("color", "#ff0000");
                $("#status_myModalLabel").html("Invalid mobile number !");
                $("#status_informationmodal").modal({
                    backdrop: 'static',
                    keyboard: false
                });
                //setTimeout($("#status_informationmodal").modal("hide"), 200000);
                return false;
            }
            else {
                $.getJSON("/Master/SendSMSForAndroidApp/" + mob, function (e) {
                    if (e == true) {
                        $("#status_myModalLabel").css("color", "#087ccd");
                        $("#status_myModalLabel").html("Message with app download link sent to  " + mob);
                        $("#customerNo").val("");
                    }
                    $("#status_informationmodal").modal({
                        backdrop: 'static',
                        keyboard: false
                    });
                    //setTimeout($("#status_informationmodal").modal("hide"), 200000);
                })
            }
            return true;
        }
    </script>

    

