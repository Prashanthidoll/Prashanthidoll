<script async src="https://www.static-contents.youth4work.com/js/bootstrap.min.js"></script>
<script type="text/javascript">

    var userid = "3849730";
    var usertype = "ST";
    var newsLn = ("").length;
    var useridforredirect;
    var catsuccess;
    var idsuccess;
    var titlesuccess;
    var itemidsuccess;
    var companynamesuccess;
    var locationnamesuccess;


    $(window).scroll(function () {
        var currentScroll = parseInt($(this).scrollTop());

        if (currentScroll > 59) {
            $(".header > .clearfix").addClass("fixedheader");
        }
        else {
            $(".header > .clearfix").removeClass("fixedheader");
        }

        if (currentScroll > 59) {
            $("#slideLeftMenu").addClass("l-menu-top");
        }
        else {
            $("#slideLeftMenu").removeClass("l-menu-top");
        }
    });

    $(function () {
        if ('True' == 'True') {
            $.get('/Master/GetActiveInfo', function (data) {
                if (data.Info != null && data.RedirectUrl != null && data.ForLang != null && data.ForLang == 'EN') {
                    $("#IdActiveInfo").css("display", "block");
                    $("#infotext").html(data.Info);
                    //$("#rediredturl").attr("href", data.RedirectUrl);
                    var html = $('#IdActiveInfo').html();
                    $('#IdActiveInfo').html('<a href="' + data.RedirectUrl + '">' + html + '</a>');
                    $("#rediredturl").html("Upgrade Now");
                }
                else {
                    $("#IdActiveInfo").css("display", "none");
                }
            });
        }
        $('.dropdown-menu').click(function (e) {
            e.stopPropagation();
        });

        $(".navbar-btn").click(function () {
            $(".left-menu").toggleClass("slideLeftMenu", 1000);
        });

        $(".left-menu > div > ul.list-unstyled > li > .accordion-toggle").click(function () {
            if ('' != "es") {
                var curr = false;
                if ($(this).parent().hasClass("active")) {
                    curr = true;
                }
                $('.panel-collapse.in').collapse('hide');
                $(".left-menu > div > ul.list-unstyled > li.active").removeClass("active");
                if (curr == false) {
                    $(this).parent().toggleClass("active");
                }
            }
        });

        $(".h-menu > li").mouseover(function () {
            var tot = parseInt($(this).offset().left) + parseInt($(this).width()) + 14;
            var top = (parseInt($(this).offset().top) + parseInt($(this).height()));
            $(this).find(".sub-menu").css("width", tot + "px").css("top", top - $(window).scrollTop() + "px");

        });

        $(".usermessages").click(function () {
            var margin = $(this).css("height");
            $(".usermessages").find(".dropdown-menu").find(".menu").html("<div style='margin: " + margin + " 0px;' align='center'><img title='loading ...' src='https://www.static-contents.youth4work.com/images/y4w-20x20-5kb.gif' align='middle'></div>");
            $.post('/Master/gety4wMessages', null, function (data) {
                $(".usermessages").find(".dropdown-menu").find(".menu").html(data);
                $.post('/Master/readMessages', null, function (data) {
                    $(".usermessages > .dropdown-toggle > .label-warning").remove();
                });
            });
        });
        $(".usernotification").click(function () {
            var margin = $(this).css("height");
            $(".usernotification").find(".dropdown-menu").find(".menu").html("<div style='margin: " + margin + " 0px;' align='center'><img title='loading ...' src='https://www.static-contents.youth4work.com/images/y4w-20x20-5kb.gif' align='middle'></div>");
            $.post('/Master/gety4wNotification', null, function (data) {
                $(".usernotification").find(".dropdown-menu").find(".menu").html(data);
                $.post('/Master/readNotifications', null, function (data) {
                    $(".usernotification > .dropdown-toggle > .label-warning").remove();
                });
            });
        });

    })


    function messagereaduser(id, userid, utype) {

        useridforredirect = userid;
        $.post('/Master/InsertintoMessageReadTable', { id: id }, function (data) {
            onSuccReadMessage(data);
        });
    }

    function onSuccReadMessage(resp) {

        if (usertype == 'ST') {
            window.location.href = 'https://www.youth4work.com/User/Inbox?v=1_' + useridforredirect;
        }
        else if (usertype == "CP") {
            window.location.href = 'https://www.youth4work.com/Company/Inbox?v=1_' + useridforredirect;
        }
    }

    function updateAndRedirectUrl(UserId, id, category, title, itemid, companyname, locationname) {

        catsuccess = category.replace(/_/g, " ");
        idsuccess = id;
        itemidsuccess = itemid;
        titlesuccess = title.replace(/_/g, " ");
        companynamesuccess = companyname.replace(/_/g, " ");
        locationnamesuccess = locationname.replace(/_/g, " ");
        $.post('/Master/InsertintoReadTable', { id: id, category: category, UserId: UserId }, function (data) {
            //onSuccReadNotificaton(data);
        });
    }
    function onSuccReadNotificaton(resp) {

        if (catsuccess == "YScore") {
            var talent = safeurl(locationnamesuccess.trim());
            window.location.href = 'https://www.youth4work.com/Talent/' + talent;
        }
        else if (catsuccess == "PREQ") {
            var projectname = safeurl(locationnamesuccess.trim());
            window.location.href = 'https://www.youth4work.com/projects/Internship-' + idsuccess + '-' + projectname + '/' + itemidsuccess + '-' + companynamesuccess;
        }
        else if (catsuccess == "Final") {
            var projectname = safeurl(locationnamesuccess.trim());
            window.location.href = 'https://www.youth4work.com/projects/Internship-' + idsuccess + '-' + projectname + '/' + itemidsuccess + '-' + companynamesuccess;
        }
        else if (catsuccess == "project") {
            var t1 = safeurl(titlesuccess.trim());
            if (titlesuccess.length > 100) {
                window.location.href = 'https://www.youth4work.com/projects/Internship-' + idsuccess + '-' + t1.Substring(0, 100);
            }
            else {
                window.location.href = 'https://www.youth4work.com/projects/Internship-' + idsuccess + '-' + t1;
            }

        }
        else if (catsuccess == "job") {
            var cn = safeurl(companynamesuccess.trim());
            var t1 = safeurl(titlesuccess.trim());
            var lc1 = safeurl(locationnamesuccess.trim());
            window.location.href = 'https://www.cos.youth4work.com/' + cn + '/jobs/' + idsuccess + '-' + t1 + '-in-' + lc1;

        }
        else if (catsuccess == "ST" || catsuccess == "TC" || catsuccess == "CP") {
            if (catsuccess == "ST") {
                window.location.href = 'https://www.youth4work.com/y/' + safeurl(companynamesuccess.replace(/ /g, "_"));
            }
            else if (catsuccess == "CP") {
                window.location.href = 'https://www.cos.youth4work.com/' + safeurl(locationnamesuccess.trim());
            }
            else if (catsuccess == "TC") {
                window.location.href = 'https://www.ed.youth4work.com/' + safeurl(companynamesuccess.replace(/ /g, "_"));
            }
        }
        else if (catsuccess == "CFollower") {
            window.location.href = 'https://www.youth4work.com/y/' + safeurl(companynamesuccess);
        }
        else if (catsuccess == "CProjectApplicants") {
            var t1 = safeurl(titlesuccess.trim());
            if (titlesuccess.length > 100) {
                window.location.href = 'https://www.youth4work.com/projects/Internship-' + itemidsuccess + '-' + t1.Substring(0, 100) + '/' + idsuccess + '-' + safeurl(companynamesuccess);
            }
            else {
                window.location.href = 'https://www.youth4work.com/projects/Internship-' + itemidsuccess + '-' + t1 + '/' + idsuccess + '-' + safeurl(companynamesuccess);
            }
        }
        else if (catsuccess == "CJobApplicants") {
            window.location.href = 'https://www.youth4work.com/Company/JobPosting';
        }
    }
    (function (i, s, o, g, r, a, m) {
        i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
            (i[r].q = i[r].q || []).push(arguments)
        }, i[r].l = 1 * new Date(); a = s.createElement(o),
       m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
    })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');
    ga('create', 'UA-16703189-2', 'youth4work.com');
    ga('require', 'displayfeatures');
    if ('true' === 'true') {
        ga('set', 'userId', '3849730'); // Set the user ID using signed-in user_id.
    }
    ga('send', 'pageview');
</script>

