    <script type="text/javascript">
        var Usertype = '';
        $(function () {
            if (Usertype == "ST") {
                $("[name=rbtnType]").val(["ST"]);
                $("#pnlOptions").hide();
                $("#spnTitle").text("Sign Up with Youth4work");
                $("#txtEmail").attr("placeholder", "Your Email");
                $("#pnlCommon").show();
            }
            else if (Usertype == "CP") {
                $("[name=rbtnType]").val(["CP"]);
                $("#pnlOptions").hide();
                $("#spnTitle").text("Sign Up with Youth4work");
                $("#txtEmail").attr("placeholder", "Your Official EmailId");
                $("#pnlCommon").show();
            }
            else if (Usertype == "TC") {
                $("[name=rbtnType]").val(["TC"]);
                $("#pnlOptions").hide();
                $("#spnTitle").text("Sign Up with Youth4work");
                $("#txtEmail").attr("placeholder", "Your Email");
                $("#pnlCommon").show();
            }
            $("#chktermcondition").click(function () {
                if ($(this).is(":checked")) {
                    $("#btnyouthregister").removeAttr("disabled");
                    $("#btnyouthregister").focus();
                } else {
                    $("#btnyouthregister").attr("disabled", "disabled");
                }
            });
            $("#chktermcondition2").click(function () {
                if ($(this).is(":checked") && CheckCompanyName()) {
                    $("#btnsubmitCP").removeAttr("disabled");
                    $("#btnsubmitCP").focus();
                } else {
                    $("#btnsubmitCP").attr("disabled", "disabled");
                }
            });
            $("#chktermcondition3").click(function () {
                if ($(this).is(":checked")) {
                    $("#btneduregister").removeAttr("disabled");
                    $("#btneduregister").focus();
                } else {
                    $("#btneduregister").attr("disabled", "disabled");
                }
            });
        });
     </script>
        <script type="text/javascript">
        function disableheader() {
            var login = '0';
            if (login == '1') {
                if ($('#pnlTalent') != undefined) {
                    $('.disable-header-menu').removeClass('hide');
                    $('.disable-bottom-menu').removeClass('hide');
                }
            }
        }
        $(function () {
            disableheader();
            $("#btnBack").on("click", function () {
                $("#pnlCommon").hide();
                $("#spnTitle").text("Type of Account you want to create");
                reset();
                $("#pnlOptions").show();
            })
            $("#txtyCourse").autocomplete({
                minLength: 2,
                autoFocus: true,
                source: "/Master/SearchCourses/"
            });
            $("#txtedCourse").autocomplete({
                minLength: 2,
                autoFocus: true,
                source: "/Master/SearchCourses/"

            });
            $("#txtySpec").autocomplete({
                minLength: 2,
                autoFocus: true,
                source: "/Master/SearchSpecialization/"
            });
            $("#txtyCollege").autocomplete({
                minLength: 2,
                autoFocus: true,
                source: "/Master/SearchCollege/"

            });
            $("#txtedCollege").autocomplete({
                minLength: 2,
                autoFocus: true,
                source: "/Master/SearchCollege/"
            });
            //$("#txtyUniversity").autocomplete({
            //    minLength: 2,
            //    autoFocus: true,
            //    source: "/Master/SearchUniversity/",
            //    search: function (event, ui) { showLoader("spnyUniversityLoader"); },
            //    response: function (event, ui) { hideLoader("spnyUniversityLoader"); }
            //});
            //$("#txtedUniversity").autocomplete({
            //    minLength: 2,
            //    autoFocus: true,
            //    source: "/Master/SearchUniversity/"
            //});

            $("#txtyLocation").autocomplete({
                minLength: 2,
                autoFocus: true,
                source: "/Master/SearchLocation/"
            });
            $("#txtEdLocation").autocomplete({
                minLength: 2,
                autoFocus: true,
                source: "/Master/SearchLocation/"
            });
            $("#txtRLocation").autocomplete({
                minLength: 2,
                autoFocus: true,
                source: "/Master/SearchLocation/"
            });
            $("#txtIndustry").autocomplete({
                minLength: 2,
                autoFocus: true,
                source: "/Master/SearchIndustry/"
            });
            $("#txtTalent").autocomplete({
                source: function (request, response) {
                    $.ajax({
                        url: "/Master/SearchTalents/",
                        data: {
                            term: request.term
                        },
                        success: function (data) {
                            if (data.length == 0) {
                                $('#btnFTerr').html("Kindly select from the dropdown");
                                $('#btnFTerr').css('display', '');
                                setTimeout(function () { $('#btnFTerr').css('display', 'none'); }, 5000);
                            }
                            response(data);

                        }
                    });
                },

                autoFocus: true,
                select: function (event, ui) {
                    event.preventDefault();
                    $(this).val('');
                    var addit = true;
                    var count = 1;
                    $("#lstTalent li span").each(function () {

                        if (ui.item.label == $(this).text().replace(count + ". ", "").trim()) {
                            addit = false;

                            $('#btnFTerr').html("Already selected talent");
                            $('#btnFTerr').css('display', '');
                            setTimeout(function () { $('#btnFTerr').css('display', 'none'); }, 5000);
                        }
                        count++;
                    });
                    if (addit)
                        $("#lstTalent").append("<li style='padding-top: 3px;padding-left: 3px;'><span>" + ($("#lstTalent li span").length + 1) + ". " + ui.item.label + "</span><a class='close'>&times;</a><input name='talents' type='hidden' value='" + ui.item.label + "' /></li>");

                    displaymsg();
                }
            });

            $("#lstTalent").on("click", "a", function () {
                $(this).parent().remove();
                displaymsg();
            });
            function displaymsg() {
                var count = $("#lstTalent li span").length;
                if (count < 3) {
                    $("#btnFT").attr("disabled", "disabled");
                }
                else {

                    $("#btnFT").removeAttr("disabled");
                }
                if (count == 0) {
                    $("#divTalent").html("Add upto 5 talents, so we can stream relevant opportunities to you.&lt;br /&gt;(eg. Content Writer/ Core Java/ Marketing/ Customer Service)");
                }
                else if (count == 1) {
                    $("#divTalent").html("Great! Now add a different talent or expertise to see more opportunities.");
                }
                else if (count == 2) {
                    $("#divTalent").html("That&#39;s 2! What other talent or expertise do you have?");
                }
                else if (count == 3) {
                    $("#divTalent").html("That&#39;s 3! You&#39;re talented, but can you think of another one?");
                }
                else if (count == 4) {
                    $("#divTalent").html("Well Done! Now add your final talent or expertise.");
                }
                else if (count == 5) {
                    $("#divTalent").html("Well Done ! Thank you!");
                }
            }

        })
        $("form").submit(function (e) {
            var ref = $(this).find("[required]");
            $(ref).each(function () {
                if ($(this).val() == '') {
                    alert("Please fill out this field..");
                    $(this).focus();
                    e.preventDefault();
                    return false;
                }
            });
            return true;
        });


    </script>
   


