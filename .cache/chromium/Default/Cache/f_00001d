        <script type="text/javascript">
            var  city, country, state, countrycode,pin;
            $(function(){
                if (navigator.geolocation) {
                    navigator.geolocation.getCurrentPosition(success, error);
                }
                else {
                    console.log("Browser doesn't support Geolocation");
                    getLatLang();
                }
                function success(position) {
                    getlocation(position.coords.latitude, position.coords.longitude);
                }
                function error(err) {
                    console.log(err.code + ":" + err.message);
                    getLatLang();
                }
                function getLatLang() {
                    $.post("https://www.googleapis.com/geolocation/v1/geolocate?key=AIzaSyDos3i57WyTW2UQvoIjUT7ddFXCULn81SE").done(function (data, status) {
                        if (status == "success") {
                            getlocation(data.location.lat, data.location.lng);
                        }
                        else {
                            console.log(status);
                            
                        }
                    }).fail(function (jqxhr, textStatus, error) {
                        var err = textStatus + ", " + error;
                        console.log("Request Failed: " + err);
                        
                    });
                }
                function getlocation(lat, lng) {
                    var GEOCODING = 'https://maps.googleapis.com/maps/api/geocode/json?key=AIzaSyDos3i57WyTW2UQvoIjUT7ddFXCULn81SE&latlng=' + lat + '%2C' + lng + '&language=en&result_type=postal_code';
                    $.getJSON(GEOCODING).done(function (location, status) {
                        if (status == "success") {
                            var obj = location.results[0].formatted_address.split(',');
                            var obj1=location.results[0].address_components;
                            if (obj !== undefined) {
                                city = obj[obj.length - 3];
                                country = obj[obj.length - 1];
                                state = obj[obj.length - 2];
                                countrycode=(obj1!==undefined && obj1.length>0)?obj1[obj1.length-1].short_name:country;
                                if (city !== undefined && country !== undefined && state !== undefined) {
                                    var objState = state.split(' ');
                                    pin = objState[objState.length - 1];
                                    state = state.replace(pin, '');
                                    $.post("/Master/SetUserLocInfo", { country: country.trim(), state: state.trim(), city: city.trim(), lat: lat, longt: lng, countrycode: countrycode.trim(),pin:pin.trim() }, function (resp) {
                                    });
                                }

                            }


                        }

                    }).fail(function (jqxhr, textStatus, error) {
                        var err = textStatus + ", " + error;
                        console.log("Request Failed: " + err);
                        
                    });
                }
            })
        </script>
