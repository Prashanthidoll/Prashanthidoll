    <script type="text/javascript">

        function translate_text(_text) {
            _text = _text.toLowerCase();
            if (_text == 'please enter the title.') {
                return "Please enter the title.";
            } else if (_text == 'please enter the company name.') {
                return "Please enter the company name.";
            } else if (_text == 'please select the type.') {
                return "Please select the type.";
            } else if (_text == 'please enter the duration.') {
                return "Please enter the duration.";
            } else if (_text == 'please select the duration.') {
                return "Please select the duration.";
            } else if (_text == 'email is not valid.') {
                return "Email is not valid.";
            } else if (_text == 'something might happened wrong, try again.') {
                return "Something might happened wrong, try again.";
            } else if (_text == 'something might happened wrong, please refresh the page.') {
                return "Something might happened wrong, please refresh the page.";
            }
        }

        function showAlert(status, message) {
            if (status == 'hide') {
                $("#model-alert .model-message").html('');
                $("#model-alert .model-close").click();
            }
            else {
                $("#model-alert .model-message").html(message);
                $("#model-alert").modal({
                    backdrop: 'static',
                    keyboard: false
                });
            }
        }
        function isValidateExtension(fileUpload, uploadFileType) {
            var allowedFiles = [".jpg", ".jpeg", ".gif", ".png", ".doc", ".docx", ".xls", ".xlsx", ".ppt", ".pptx", ".pdf", ".txt", ".zip", ".rar"];
            if (uploadFileType == "pic") {
                allowedFiles = [".jpg", ".jpeg", ".gif", ".png"];
            }
            else if (uploadFileType == "doc") {
                allowedFiles = [".doc", ".docx", ".pdf", ".txt", ".zip", ".rar", ".xlsx", ".ppt"];
            }

            return true;
        }

        function hidegoogleTag() {
            $("input.gsc-input").css("font-size", "12px");
            $("input.gsc-input").attr("placeholder", "Search for Online Tests, Jobs, People, Ranked Talent Board");
            $("input.gsc-input").css("background", "none");
        }
        function showImagePopup(itm) {
            var srcPath = $(itm).attr("src");
            $("#my-origin-image").attr("src", srcPath);

            $("#profileImg").modal({
                backdrop: 'static',
                keyboard: false
            });
        }

        function CheckLogin() {
            if ('False' == 'True') {
                return true;
            }
            else {
                $("#MasterLogin").modal({
                    backdrop: 'static',
                    keyboard: false
                });
                return false;
            }
        }

