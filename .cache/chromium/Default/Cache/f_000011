<script src="/js/jquery-ui-1.10.4.custom.min.js"></script>
<script src="/js/talentcombo.js" type="text/javascript"></script>
<link href="https://www.static-contents.youth4work.com/Styles/checkBcombo.css" rel="stylesheet" type="text/css" />
    <script type="text/javascript">
        var irepeat = 1;
        var fn = "";
        var currField = "TalentSearch";
        $(window).resize(function () {
            $("#logoImg").parent().parent().parent().css("height", $("#logoImg").height() + "px");
        })

        function ShowModelFilterList() {
            $("#modelFilterList").modal({
                backdrop: 'static',
                keyboard: false
            });
        }
               
        $(function () {
            $('.search-textbox').hide();
            $('#TalentSearch').show();
            $('.input-search-btn').hover(function () {
                if ($(window).width() > 420) { $(this).find('.main-menu').stop(true, true).delay(200).fadeIn(500); }
            }, function () {
                if ($(window).width() > 420) { $(this).find('.main-menu').stop(true, true).delay(200).fadeOut(500); }
            });
            $('.browseCategory').click(function () {
                var newText = $(this).find('span').html();
                $('#browseCategory .ddltext').html(newText);
                var type = $(this).attr('data-searchdiv');
                var divid = $(this).attr('data-searchdivid');
                currField = divid;
                $('#cType').val(type);
                $('.search-textbox').hide();
                $('#' + divid).show();
                $('#' + divid).focus();
                $(this).parent().parent().hide();
            })
            $('#ddlbrowseCategory').change(function () {
                var type = $(this).val();
                var divid = type;
                if (type == 'Specialization') {
                    divid = 'SpecSearch';
                } else if (type == 'Prepares') {
                    divid = 'PrepSearch';
                } else {
                    divid = divid + 'Search';
                }

                $('#cType').val(type);
                $('.search-textbox').hide();
                $('#' + divid).show();
                $('#' + divid).focus();
            })
            $("#TalentSearch").autocomplete({
                minLength: 2,
                source: "/Home/SearchTalents/",
                response: function (event, ui) {
                    if (ui.content.length == 0) {
                        ui.content.push({ label: "No Match Found", value: "" });
                    }
                },
                select: function (event, ui) {
                    if (ui.item.value != "") {
                        event.preventDefault();
                        $(this).val('');
                        bindListData(ui.item.label, 'talents');
                    }
                }
            });
            $("#LocationSearch").autocomplete({
                minLength: 2,
                source: "/Master/SearchLocations",
                response: function (event, ui) {
                    if (ui.content.length == 0) {
                        ui.content.push({ label: "No Match Found", value: "" });
                    }
                },
                select: function (event, ui) {
                    if (ui.item.value != "") {
                        event.preventDefault();
                        $(this).val('');
                        bindListData(ui.item.label, 'locations');
                    }
                }
            });
            $("#CourseSearch").autocomplete({
                minLength: 2,
                source: "/Master/SearchCourses",
                response: function (event, ui) {
                    if (ui.content.length == 0) {
                        ui.content.push({ label: "No Match Found", value: "" });
                    }
                },
                select: function (event, ui) {
                    if (ui.item.value != "") {
                        event.preventDefault();
                        $(this).val('');
                        bindListData(ui.item.label, 'courses');
                    }
                }
            });
            $("#SpecSearch").autocomplete({
                minLength: 2,
                source: "/Master/SearchSpecialization",
                response: function (event, ui) {
                    if (ui.content.length == 0) {
                        ui.content.push({ label: "No Match Found", value: "" });
                    }
                },
                select: function (event, ui) {
                    if (ui.item.value != "") {
                        event.preventDefault();
                        $(this).val('');
                        bindListData(ui.item.label, 'specializations');
                    }
                }
            });
            $("#PrepSearch").autocomplete({
                minLength: 2,
                source: "/Master/SearchPrep",
                response: function (event, ui) {
                    if (ui.content.length == 0) {
                        ui.content.push({ label: "No Match Found", value: "" });
                    }
                },
                select: function (event, ui) {
                    if (ui.item.value != "") {
                        event.preventDefault();
                        $(this).val('');
                        bindListData(ui.item.label, 'preps');
                    }
                }
            });
        })
        function bindListData(item, flag) {
            var addit = true;
            var li_id = $(".filterdataList li").length;
            if ($(window).width() < 430) {

            }

            $(".filterdataList li." + flag + "class span").each(function () {
                if (item == $(this).text()) {
                    addit = false;
                }
            });
            if (addit) {
                $(".filterdataList").append("<li data-id='" + li_id + "' class='id" + li_id + " label label-default " + flag + "class'><span title='" + item + "'>" + item + "</span>&nbsp;<a class='close' onclick='closeli(this)'>&times;</a><input name='" + flag + "' type='hidden' value='" + item + "' /></li>");
              //  $(".filterdataList").append('<li data-id="' + li_id + '" class="id' + li_id + ' filters ' + flag + '"><div class="clearfix"><div class="ftext" title="' + item + '">' + item + '</div><div class="closebtn pull-right"><a class="close" onclick="closeli(this)">x</a></div><input name="' + flag + '" type="hidden" value="' + item + '" /></div></li>');
            }

            showdisableDivMsg();
        }
        function showdisableDivMsg() {
            if ($(window).width() < 420) {
                if ($("#dataList li").length > 2) {
                    if (!$('#disableDiv').hasClass('disableDivshowmsg')) {
                        $('#disableDiv').addClass('disableDivshowmsg');
                    }
                    $('#disableDiv').html('Click here to see all filter.');
                } else {
                    if ($('#disableDiv').hasClass('disableDivshowmsg')) {
                        $('#disableDiv').removeClass('disableDivshowmsg');
                    }
                    $('#disableDiv').html('');
                }
            }
        }

        function closeli(curr) {            
            if ($(curr).parent().parent().attr('id') == 'mobdataList') {
                var dataid = $(curr).parent().attr('data-id');
                 $('#dataList').find('.id' + dataid).remove();               
                showdisableDivMsg();
            }
            $(curr).parent().remove();
        }


        $(window).scroll(function (event) {
            var scrollPos = $(window).scrollTop();
            var width = $(window).width();
            if (width > 400) {
                if (scrollPos > 50 && scrollPos < 100) {
                    $(".find-pre-assessed-profile").addClass("box-moveFromLeft");;
                } else if (scrollPos > 100 && scrollPos < 150) {
                    $(".find-pre-assessed-profile .transparent").addClass("box-moveFromBottom");;
                } else if (scrollPos > 620 && scrollPos < 660) {
                    $(".site-data").addClass("box-moveFromBottom");
                } else if (scrollPos > 920 && scrollPos < 960) {
                    $(".our-aim").addClass("box-moveFromBottom");
                } else if (scrollPos > 1420 && scrollPos < 1460) {
                    $(".pg-container").addClass("box-moveFromBottom");
                } else if (scrollPos > 1620 && scrollPos < 1660) {
                    $(".finders").addClass("box-moveFromBottom");
                }
            }
        });
