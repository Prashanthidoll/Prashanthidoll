        // developed By Amit
        var flag = 0;
        var tim;
        var oldAttempt = 0;
        var newAttempt = 0;
        var oldAttemptLocation = 0;
        var newAttemptLocation = 0;
        var oldTest = 0;
        var newTest = 0;
        var oldprofile = 0;
        var newprofile = 0;
        var oldCos = 0;
        var newCos = 0;
        function getHomeDetail() {
            $.getJSON("Home/getStats", function (data) { onSuc(data) });
            setTimeout("getHomeDetail()", 20000);
        };

        function onSuc(resp) {
            var data = resp;
            if (data != null) {
                oldAttempt = parseInt(data.TotalQuestionAttempted) - 5;
                oldAttemptLocation = parseInt(data.TotalLocationTestAttempted) - 5;
                oldTest = parseInt(data.TotalTest) - 5;
                oldprofile = parseInt(data.TotalYouth + data.TotalEmployer + data.TotalEducator) - 5;
                oldCos = parseInt(data.TotalEmployer) - 5;

                newAttempt = parseInt(data.TotalQuestionAttempted)
                newAttemptLocation = parseInt(data.TotalLocationTestAttempted);
                newTest = parseInt(data.TotalTest);
                newprofile = parseInt(data.TotalYouth + data.TotalEmployer + data.TotalEducator);
                newCos = parseInt(data.TotalEmployer);

                $("#lbltotalAttempts").html(addCommas(newAttempt));
                $("#lbltotalAttemptLocation").html(addCommas(newAttemptLocation));
                $("#lbltotalProfile").html(addCommas(newprofile));
                //$("#lbltotalProfile1").html(addCommas(newprofile));
                $("#lbltotalCos").html(addCommas(newCos));

                f2(parseInt(oldAttempt), parseInt(newAttempt), "lbltotalAttempts");
                f2(parseInt(oldAttemptLocation), parseInt(newAttemptLocation), "lbltotalAttemptLocation");
                f2(parseInt(oldprofile), parseInt(newprofile), "lbltotalProfile");
                f2(parseInt(oldprofile), parseInt(newprofile), "lbltotalProfile1");
                f2(parseInt(oldCos), parseInt(newCos), "lbltotalCos");
            }
        }
        function addCommas(nStr) {
            nStr += '';
            x = nStr.split('.');
            x1 = x[0];
            x2 = x.length > 1 ? '.' + x[1] : '';
            var rgx = /(\d+)(\d{3})/;
            while (rgx.test(x1)) {
                x1 = x1.replace(rgx, '$1' + ',' + '$2');
            }
            return x1 + x2;
        }
        function f2(From, To, ctl) {
            From = parseInt(From) + 1;
            $("#" + ctl).html(addCommas(From.toString()));
            tim = setTimeout("f2('" + From + "','" + To + "','" + ctl + "')", 150);
            if (parseInt(From) == parseInt(To)) {
                $("#" + ctl).html(addCommas(From.toString()));
                clearTimeout(tim);
                return;
            }
        }
        function ShowImage1(id) {
            id.style.backgroundImage = 'url(/img/autLoad.gif)';
            id.style.backgroundRepeat = 'no-repeat';
            id.style.backgroundPosition = 'right';
        }
        function ShowImage() {
            $("#Test").css("backgroundImage", 'url(/img/autLoad.gif)');
            $("#Test").css("backgroundRepeat", "no-repeat");
            $("#Test").css("backgroundPosition", "right");
        }
        function HideImage() {
            $("#Test").css("backgroundImage", 'none');
        }
        function moveFocus(event, obj) {
            var $this = $(obj);
            if (event.keyCode == 38 && $this.prev() != null) {
                event.preventDefault();
                $this.prev().focus();
            }
            else if (event.keyCode == 40 && $this.next() != null) {
                event.preventDefault();
                $this.next().focus();
            }
            else if (event.keyCode == 32 || event.keyCode == 37 || event.keyCode == 39 || event.keyCode == 46) {
                event.preventDefault();
                $this.click();

            }
        }

        function datashow(txt) {
            clearInterval(fn);
            for (var i = 1; i <= 5; i++) {
                $("#div" + i).hide();
                $("#span" + i).hide();
                $("#divhover" + i).find('div.y4w-module').css('opacity', '0.3');
            }
            $("#div" + txt).show();
            $("#span" + txt).show().find('span').css('margin-left', '0');
            $("#divhover" + txt).find('div').css('opacity', '1');
            irepeat = txt;
        }
        function initateTimer() {
            fn = setInterval("startTimer()", 3000);
        }
        function startTimer() {
            for (var i = 1; i <= 5; i++) {
                $("#div" + i).hide();
                $("#span" + i).hide();
                $("#divhover" + i).find('div.y4w-module').css('opacity', '0.3');
            }
            $("#div" + irepeat).show();
            $("#span" + irepeat).show().find('span').css('margin-left', '0');
            $("#divhover" + irepeat).find('div').css('opacity', '1');
            if (irepeat < 5) {
                irepeat++;
            }
            else {
                irepeat = 1;
            }

        }
