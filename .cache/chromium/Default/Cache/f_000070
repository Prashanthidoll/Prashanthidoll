    <script type="text/javascript">
        $(function () {
            $.get("/GetLanguageForDropdown", function (data) {
                if (data != "") {
                    $("#dropdownlist").html(data);
                    $("#dropdownlist > ul").prepend('<li style="vertical-align: middle;"><span class="icons-31 icons-31-lang"></span></li>');
                }
            });
        })
        $(window).scroll(function () {
            var mobileView = $(this).width();
            var top = "0px";
            if (mobileView < 480) {
                top = "50px";
            }
            else if (mobileView > 481 && mobileView < 781)
            {
                top = "55px";
            }
            var h = window.pageYOffset;
            if (h > 300) {
                $("#topdiv").css("position", "fixed");
                $("#topdiv").css("top", top);
            }
            else {
                $("#topdiv").css("position", "");
                $("#topdiv").css("top", "-10px");
            }
        })
        function ShowtestDiv() {
            $('html, body').animate({ scrollTop: 500 }, 1000);
        }
        function Showtest() {
            $('html, body').animate({ scrollTop: 300 }, 1000);
        }
        function Showanalysis() {
            $('html, body').animate({ scrollTop: 2500 }, 1000);
        }
        function Showf() {
            $('html, body').animate({ scrollTop: 3250 }, 1000);
        }
        function ShowSeemore(id) {
            $("#" + id).show();
        }
        function HideSeemore(id) {
            $("#" + id).hide();
        }
        function handleSelect(elm) {
            window.location = "/" + elm.value;
        }
    </script>

